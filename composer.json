{
  "name": "alex-kalanis/redis-wrapper",
  "description": "PHP wrapper for questioning Redis as file storage",
  "keywords": [
    "redis",
    "predis",
    "wrapper",
    "wrappers",
    "storage"
  ],
  "license": "BSD-3-Clause",
  "type": "library",
  "require": {
    "php": ">=7.4.0"
  },
  "require-dev": {
    "alex-kalanis/kw_storage": ">=5 <6",
    "predis/predis": "^2.2",
    "ext-redis": "*",
    "shipmonk/composer-dependency-analyser": "^1.4",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpunit/phpunit": ">=8 <=9"
  },
  "autoload": {
    "psr-4": {
      "kalanis\\RedisWrapper\\": "src/"
    }
  },
  "suggest": {
    "ext-redis": "Direct php library for connection to your Redis server",
    "predis/predis": "External library for connecting your Redis cluster",
    "alex-kalanis/kw_storage": "Universal storage libraries"
  },
  "scripts": {
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}